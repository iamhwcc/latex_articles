POST flights_test/_search
{
    "query": {
        "match_all": {}
    },
    "size": 0,
    "aggs": {
        "date": {
            "date_histogram": {
                "field": "timestamp",
                "fixed_interval": "7d"
            },
            "aggs": {
                "price_avg": {
                    "avg": {
                        "field": "AvgTicketPrice"
                    }
                }
            }
        },
        "avgprice_avg": {
            "avg_bucket": {
                "buckets_path": "date>prive_avg"
            }
        }
    }
}